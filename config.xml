<?xml version="1.0" encoding="UTF-8" ?>
<widget id="com.f5.spacewalker" version="1.0.0.0" android-versionCode="1000000" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>SpaceWalker</name>
	<description>Using real footstep as game energy to discover different planets</description>
	<author></author>

	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />

	<icon src="www/icons/icon-512.png" width="512" height="512" density="xxxhdpi" />
	<icon src="www/icons/icon-256.png" width="256" height="256" />
	<icon src="www/icons/icon-128.png" width="128" height="128" density="xhdpi" />
	<icon src="www/icons/icon-64.png" width="64" height="64" density="mdpi" />
	<icon src="www/icons/icon-32.png" width="32" height="32" />
	<icon src="www/icons/icon-16.png" width="16" height="16" />

	<plugin name="cordova-plugin-inappbrowser" />
	<plugin name="cordova-plugin-whitelist" />
	<plugin name="cordova-plugin-file" />
	<plugin name="cordova-plugin-statusbar" />
	<plugin name="cordova-custom-config" />
	<preference name="orientation" value="portrait" />
	<preference name="fullscreen" value="true" />
	<preference name="DisallowOverscroll" value="true" />
	<preference name="LoadUrlTimeoutValue" value="120000" />
	<access origin="*" />

	<engine name="android" />

	<platform name="android">
		<preference name="android-minSdkVersion" value="24" />
		<preference name="android-targetSdkVersion" value="26" />
	</platform>

	<preference name="phonegap-version" value="cli-8.0.0" /></widget>